<?xml version="1.0"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.ebank.com.ebank">

    <!--允许联网 -->


    <uses-permission android:name="android.permission.INTERNET" />

    <!--获取GSM（2g）、WCDMA（联通3g）等网络状态的信息 -->


    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <!--获取wifi网络状态的信息 -->


    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <!--保持CPU 运转，屏幕和键盘灯有可能是关闭的,用于文件上传和下载 -->


    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <!--获取sd卡写的权限，用于文件上传和下载-->


    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!--允许读取手机状态 用于创建BmobInstallation-->


    <uses-permission android:name="android.permission.READ_PHONE_STATE" />


    -
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name=".activity.Index_activity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">


            <intent-filter>

                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>

        </activity>

        <activity android:name=".activity.Main_Activity" />

        <activity android:name=".activity.YuE_activity" />

        <activity android:name=".activity.ZhangDan_activity" />

        <activity android:name=".activity.Gerenxinxi_activity" />

        <activity android:name=".activity.ZhuanZhang_activity" />

        <activity android:name=".activity.SignUp_activity" />

        <activity android:name=".activity.SetMsg_activity" />

        <activity android:name=".activity.ToSetBuyPsw" />

        <activity android:name=".activity.Transfer_activity" />

        <activity android:name=".activity.Recharge_activity" />

        <activity android:name=".activity.ForgetPsw_activity" />

        <activity android:name=".activity.CheckData" />
        <activity android:name=".activity.ToSetBuyPsw2"></activity>

    </application>

</manifest>